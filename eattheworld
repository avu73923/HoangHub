local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/jensonhirst/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "eat the word", HidePremium = false, SaveConfig = true, ConfigFolder = "Việt Hoàng ios"})

OrionLib:MakeNotification({
	Name = "Việt Hoàng ios",
	Content = "",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local MainTab = Window:MakeTab({
	Name = "Auto Grap & eat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddToggle({
	Name = "This is a toggle!",
	Default = false,
	Callback = function(Value)
		print(Value)

while true do

local args = {
    [1] = false,
    [2] = false
}

game:GetService("Players").LocalPlayer.Character.Events.Grab:FireServer(unpack(args))

game:GetService("Players").LocalPlayer.Character.Events.Eat:FireServer()
wait(1)
end
})
